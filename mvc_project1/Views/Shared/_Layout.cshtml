<!DOCTYPE html>
<html lang="en" data-ng-app="app">
    <head>
        <meta charset="utf-8" />
        <title> Degreed Coding Challenge</title>
        <link href="~/favicon.ico" rel="shortcut icon" type="image/x-icon" />
        <meta name="viewport" content="width=device-width" />
        @Styles.Render("~/Content/css")
        <link href="~/Content/boostrap/css/bootstrap.min.css" rel="stylesheet" />
        <link href="~/Content/boostrap/css/bootstrap-theme.min.css" rel="stylesheet" />



        @Scripts.Render("~/bundles/modernizr")
    </head>
    <body>
        <header>
            <div class="content-wrapper">
                <div class="float-left">
                    <p class="site-title">@Html.ActionLink("Degreed Coding Challenge", "Index", "Home")</p>
                </div>
                <div class="float-right">
                    <section id="login">
                        @Html.Partial("_LoginPartial")
                    </section>
                   
                </div>
            </div>
        </header>
        <div id="body">
            @RenderSection("featured", required: false)
            <section class="content-wrapper main-content clear-fix">
                @RenderBody()
            </section>
        </div>
        <footer>
            <div class="content-wrapper">
                <div class="float-left">
                    <p>&copy; @DateTime.Now.Year - andrea.thacker</p>
                </div>
            </div>
        </footer>




        <!--Javascript to be included on every page... -->




        <script src="~/Scripts/jquery-1.9.1.min.js"></script>

        <script src="~/Scripts/jquery-ui-1.8.24.js"></script>
        <script src="~/Scripts/bootstrap.min.js"></script>
        <script src="~/Scripts/angular.min.js"></script>

        <script src="~/js/app.js"></script>

        <script src="~/js/feedback/feedbackController.js"></script>
        <script src="~/js/feedback/feedbackService.js"></script>

        @Scripts.Render("~/bundles/jquery")
        



        @RenderSection("scripts", required: false)
    </body>
</html>
